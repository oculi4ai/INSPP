}

/* ############ color 4 ############# */

QLineEdit , QPushButton , QToolButton , QListWidget , QPushButton , QSpinBox ,  QComboBox  , QDateEdit
,#frame_7 ,#frame_9,QTextEdit,QProgressBar,QTableView,QToolTip
{border: 2px solid #2F5D62;
}


QTableWidget{
	gridline-color: 2px solid #2F5D62;

}

