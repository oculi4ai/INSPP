color:rgb(0,0,0);
border-color:#5E8B7E;

}


#tab_3, #tab_4, #tab_5, #tab_6{
    
	border-radius: 20px;
}


QToolTip{
background-color: #ffffff ;
color:#2F5D62;
border-color:#ffffff;

}

QTableView::item:selected{
	color: rgb(200,200,200);
	background-color:#5E8B7E;

}

QProgressBar::chunk:horizontal{
	background-color:#3fc1b0;}

#notifications_menu{
	background-color: #f1f1f1;
	padding:10px;
}

#notifications_menu::item{
		background-color: #3fc1b0 ;
		color: rgb(20,20,20);
		margin:10px;
		margin-bottom:0px;
		padding:15px;
		border-radius: 3px;
		border-top-right-radius: 0px;

}


QCheckBox::indicator:unchecked
{
background-color: rgba(0,0,0,0);
border:2px solid #3fc1b0;
border-radius: 3px;
}
QCheckBox::indicator:checked
{
background-color: #3fc1b0;
border:2px solid #3fc1b0;
border-radius: 3px;
}
QRadioButton::indicator:unchecked{
border-radius: 7px;
background-color: rgba(0,0,0,0);
border:2px solid #3fc1b0;
}
QRadioButton::indicator:checked{
border-radius: 7px;
background-color: #3fc1b0;
border:2px solid #3fc1b0;
}

 QComboBox QAbstractItemView{
	background:#2F5D62;
	border-radius: 3px;
	border-width:0px;
}

#menuExport, #menuThemes, #menuLanguages{
	color:rgb(200,200,200);
